{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api",
      "methods": ["OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Accept, Origin",
        "Access-Control-Max-Age": "86400"
      },
      "status": 204
    },
    {
      "src": "/api",
      "dest": "index.js",
      "methods": ["POST"]
    }
  ]
}
